        "guzzlehttp/guzzle": "^7.3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bitsika\\BinanceSdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Muheez Jimoh",
            "email": "jamuheez2009@gmail.com"
        },
        {
            "name": "Tommie Darku",
            "email": "tombel24@gmail.com"
        }
    ],
    "minimum-stability": "dev",
